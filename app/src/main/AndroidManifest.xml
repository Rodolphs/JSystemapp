<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="br.com.jeguesbar.appjsystemapp">
	
	<uses-permission android:name="android.permission.INTERNET"/>
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
	
	<application
			android:name=".LMSApplication"
			android:allowBackup="true"
			android:icon="@drawable/apresentacao_jegues_bar_system"
			android:label="@string/app_name"
			android:roundIcon="@drawable/apresentacao_jegues_bar_system"
			android:supportsRtl="true"
			android:theme="@style/AppTheme">
		<activity android:name=".ProdutoActivity">
		</activity>
		<activity android:name=".SolicitacaoActivity">
		</activity>
		<activity android:name=".CancelarPedidoActivity">
		</activity>
		<activity android:name=".PedidosActivity">
		</activity>
		<activity
				android:name=".MainActivity"
				android:theme="@style/AppTheme">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity android:name=".DebugActivity"/>
		<activity
				android:name=".TelaInicialActivity"
				android:theme="@style/AppTheme.Toolbar"/>
	</application>

</manifest>